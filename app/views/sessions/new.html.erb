<h1>Sign in</h1>

<div>
	<%= form_for(:session, url: sessions_path) do |f| %>

		<%= f.label :name %>
		<%= f.text_field :name %>

		<%= f.label :password %>
		<%= f.password_field :password %>

		<%= f.submit "Sign in" %>

	<%end%>
	<p>New Cosmetician ?<%= link_to "Sign up Now", signup_path %></p>
</div>
