<li id="<%= feed_item.id %>">
	<span>
		<%= link_to feed_item.name, :controller => "wanted_lists", :action => "show" %>
		<% if current_cosmetician?(feed_item.cosmetician) %>
			<%= link_to "delete", feed_item, method: :delete,
				data: { confirm: "You sure?" },
				title: feed_item.name %>
		<% end %>
	</span>
</li>

